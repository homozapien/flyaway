<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>flyaway</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>LoginController</display-name>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>com.flyaway.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>com.flyaway.init.FlyAwayServletContextListner</listener-class>
  </listener>
  <servlet>
    <description></description>
    <display-name>CityAirportController</display-name>
    <servlet-name>CityAirportController</servlet-name>
    <servlet-class>com.flyaway.controller.CityAirportController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CityAirportController</servlet-name>
    <url-pattern>/CityAirportController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AirlineController</display-name>
    <servlet-name>AirlineController</servlet-name>
    <servlet-class>com.flyaway.controller.AirlineController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AirlineController</servlet-name>
    <url-pattern>/AirlineController</url-pattern>
  </servlet-mapping>
</web-app>